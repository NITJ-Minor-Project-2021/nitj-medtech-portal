<script>
  document.getElementById("diseaseForm").addEventListener("submit", function(event) {
    event.preventDefault(); // Prevent the default form submission behavior

    // Get the form data
    const formData = new FormData(this);

    // Send an AJAX request to predict the disease
    fetch("/predictions", {
      method: "POST",
      body: formData
    })
    .then(response => {
      if (!response.ok) {
        throw new Error("Network response was not ok");
      }
      return response.json(); // Assuming the response is JSON
    })
    .then(data => {
      // Update the UI with the predicted disease
      document.getElementById("result").innerHTML = `
        <h1>${data.disease}</h1>
        <p>Please visit a doctor for further evaluation and treatment.</p>
      `;
    })
    .catch(error => {
      console.error("Error:", error);
    });
  });
</script>

  

